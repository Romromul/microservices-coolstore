{
  "API_VERSION": "1.0",
  "SERVICE_VERSION": "0.0.1",
  "QualifiedAssemblyPattern": "VND.CoolStore.Services.*",
  "Hosts": {
    "k8s": false, 
    "ServiceName": "review_service",
    "Local": {
      "Host": "0.0.0.0",
      "Port": "5006"
    }
  },
  "Features": {
    "Mongo": {
      "ConnString": "mongodb://127.0.0.1:27017",
      "Database": "reviews"
    }
  },
  "Idp": {
    "Authority": "http://localhost:8085",
    "Audience": "api"
  },
  "Logging": {
    "IncludeScopes": true,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  }
}
