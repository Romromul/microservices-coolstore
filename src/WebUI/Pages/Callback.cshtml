@page "/callback"
@inject AppState state
@inject JsInterop jsInterop

<NavLink href="/">
  Back to home
</NavLink>
@functions {

  protected override async Task OnInitAsync()
  {
    var user = await jsInterop.Callback();
    if (user != null)
    {
      await state.UpdateUser(user);
      await jsInterop.Navigate("/");
    }
  }

}
