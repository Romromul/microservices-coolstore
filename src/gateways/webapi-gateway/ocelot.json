{
  "ReRoutes": [
    {
      "UpstreamPathTemplate": "/api/carts/{cartId}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/coolstore.ShoppingCart/GetCart",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shopping_cart_api",
          "Port": 15003
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/carts",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/coolstore.ShoppingCart/InsertItemToNewCart",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shopping_cart_api",
          "Port": 15003
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/carts",
      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamPathTemplate": "/coolstore.ShoppingCart/UpdateItemInCart",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shopping_cart_api",
          "Port": 15003
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/carts/{cartId}/checkout",
      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamPathTemplate": "/coolstore.ShoppingCart/Checkout",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shopping_cart_api",
          "Port": 15003
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/carts/{cartId}/items/{productId}",
      "UpstreamHttpMethod": [ "Delete" ],
      "DownstreamPathTemplate": "/coolstore.ShoppingCart/DeleteItem",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shopping_cart_api",
          "Port": 15003
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/products/{currentPage}/{highPrice}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/coolstore.Catalog/GetProducts",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "product_catalog_api",
          "Port": 15002
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/products/{productId}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/coolstore.Catalog/GetProductById",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "product_catalog_api",
          "Port": 15002
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/products",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/coolstore.Catalog/CreateProduct",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "product_catalog_api",
          "Port": 15002
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/products/{productId}",
      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamPathTemplate": "/coolstore.Catalog/UpdateProduct",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "product_catalog_api",
          "Port": 15002
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/products/{productId}",
      "UpstreamHttpMethod": [ "Delete" ],
      "DownstreamPathTemplate": "/coolstore.Catalog/DeleteProduct",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "product_catalog_api",
          "Port": 15002
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "QuotaExceededMessage": "Hey, you request too many times, slow down!!!",
      "HttpStatusCode": 999
    }
  }
}
